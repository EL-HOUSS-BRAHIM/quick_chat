{
  "source": {
    "include": ["assets/js"],
    "exclude": ["assets/js/dist", "node_modules"],
    "includePattern": ".+\\.js$"
  },
  "plugins": ["plugins/markdown"],
  "opts": {
    "destination": "./docs/js",
    "recurse": true,
    "readme": "README.md",
    "template": "node_modules/docdash"
  },
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true
  },
  "docdash": {
    "static": true,
    "sort": true,
    "sectionOrder": [
      "Classes",
      "Modules",
      "Externals",
      "Events",
      "Namespaces",
      "Mixins",
      "Tutorials",
      "Interfaces"
    ],
    "search": true,
    "collapse": true,
    "typedefs": true,
    "removeQuotes": "none",
    "scripts": [],
    "menu": {
      "Github repo": {
        "href": "https://github.com/yourusername/quick-chat",
        "target": "_blank",
        "class": "menu-item",
        "id": "github_link"
      }
    }
  }
}
